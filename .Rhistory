calcPredictionError <-
function (gp, newData, nugget = 0)
{
print('new ish')
r = calcCorOneObs(gp$X, gp$beta, gp$a, newData)
v = gp$sig2 + nugget - gp$sig2 * r %*% gp$invVarMatrix %*%
t(r) * gp$sig2
print((1- sum(solve(gp$invVarMatrix,r)))^2 / (sum(gp$invVarMatrix)))
if (v < 0)
return(0)
return(sqrt(v))
}
calcPredictionError(mod,X)
